                                "options": {
                                    "selectors": {
                                        "identificationNumber": ".csc-acquisition-numberPatternChooser-reference-number" 
                                    },
                                    "uispec": "{pageBuilder}.options.uispec.details"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
