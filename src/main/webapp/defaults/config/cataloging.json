{
    "pageBuilder": {
        "options": {
            "pageType": "cataloging",
            "selectors": {
                "recordEditor": ".csc-cataloging-template"
            },
            "components": {
                "recordEditor": {
                    "type":  "cspace.recordEditor",
                    "options": {
                        "selectors": {
                            "identificationNumber": ".csc-object-identification-object-number" 
                        },
                        "strings": {
                            "identificationNumberRequired": "Please specify an Identification Number" 
                        },
                        "applier": "{pageBuilder}.applier",
                        "uispec": "{pageBuilder}.options.uispec.recordEditor",
                        "model": "{pageBuilder}.model"
                    }
                },
                "titleBar": {
                    "type": "cspace.titleBar",
                    "options": {
                        "fields": [
                            "fields.objectNumber", {
                            "type": "repeatableMatch",
                            "queryPath": "fields.titleGroup",
                            "childPath": "_primary",
                            "value": true,
                            "path": "title"
                        }]
                    }
                },
                "header": {
                    "type": "cspace.header",
                    "options": {
                        "schema": "{pageBuilder}.schema",
                        "permissions": "{pageBuilder}.permissions"
                    }
                },
                "tabs": {
                    "type": "cspace.tabs"
                },
                "sidebar": {
                    "type": "cspace.sidebar"
                }
            }
        }
    },
    "pageBuilderIO": {
        "options": {
            "recordType": "cataloging",
             "schema": [
                "recordlist",
                "recordtypes",
                "namespaces",
                "cataloging",
                "reporting"
            ],
            "pageSpec": {
                "recordEditor": {
                    "href": "%readonlypages/CatalogingTemplate.html",
                    "templateSelector": ".csc-cataloging-template",
                    "targetSelector": ".csc-record-edit-container" 
                }
            }
        }
    }
}