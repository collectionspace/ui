{
    "pageBuilderIO": {
        "options": {
            "recordType": "movement",
             "schema": [
                "recordlist",
                "namespaces",
                "recordtypes",
                "movement"
            ],
            "pageSpec": {
                "details": {
                    "href": "pages/MovementTemplate.html",
                    "templateSelector": ".csc-movement-template",
                    "targetSelector": ".csc-relatedRecordsTab-movement .csc-relatedRecordsTab-recordEditor"
                } 
            }
        }
    },
    "pageBuilder": {
        "options": {
            "pageType": "movement-tab",
            "selectors": {
                "relatedRecordsTab": ".csc-relatedRecordsTab-movement"
            },
            "components": {
                "relatedRecordsTab": {
                    "type": "cspace.relatedRecordsTab",
                    "options": {
                        "primary": "{pageBuilder}.options.primary",
                        "related": "{pageBuilder}.options.related",
                        "applier": "{pageBuilder}.applier",
                        "model": "{pageBuilder}.model",
                        "uispec": "{pageBuilder}.options.uispec",
                        "strings": {
                            "editRecord": "Edit Location/Movement/Inventory Control Record",
                            "recordList": "Related Location/Movement/Inventory Control Records",
                            "newRecordRow": "New Location/Movement/Inventory Control Record"
                        },
                        "components": {
                            "listEditor": {
                                "options": {
                                    "components": {
                                        "details": {
                                            "options": {
                                                "selectors": {
                                                    "identificationNumber": ".csc-movement-currentLocation" 
                                                } 
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}