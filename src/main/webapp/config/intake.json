{
    "pageBuilder": {
        "options": {
            "pageType": "intake",
            "selectors": {
                "recordEditor": ".csc-intake-template"
            },
            "components": {
                "recordEditor": {
                    "type":  "cspace.recordEditor",
                    "options": {
                        "selectors": {
                            "identificationNumber": ".csc-intake-entry-number" 
                        },
                        "strings": {
                            "identificationNumberRequired": "Please specify an Intake Entry Number" 
                        },
                        "applier": "{pageBuilder}.applier",
                        "uispec": "{pageBuilder}.options.uispec.recordEditor",
                        "model": "{pageBuilder}.model"
                    }
                },
                "titleBar": {
                    "type": "cspace.titleBar",
                    "options": {
                        "fields": [
                            "fields.entryNumber",
                            "fields.currentOwner" 
                        ]
                    }
                },
                "header": {
                    "type": "cspace.header",
                    "options": {
                        "schema": "{pageBuilder}.schema",
                        "permissions": "{pageBuilder}.permissions"
                    }
                },
                "tabs": {
                    "type": "cspace.tabs"
                },
                "sidebar": {
                    "type": "cspace.sidebar",
                    "options": {
                        "uispec": "{pageBuilder}.options.uispec.sidebar"
                    }
                }
            }
        }
    },
    "pageBuilderIO": {
        "options": {
            "recordType": "intake",
             "schema": [
                "recordlist",
                "recordtypes",
                "intake"
            ],
            "pageSpec": {
                "recordEditor": {
                    "href": "%readonlypages/IntakeTemplate.html",
                    "templateSelector": ".csc-intake-template",
                    "targetSelector": ".csc-record-edit-container"
                } 
            }
        }
    }
}