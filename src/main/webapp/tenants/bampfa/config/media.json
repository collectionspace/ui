{
	"pageBuilder": {
		"options": {
			"pageType": "media",
			"components": {
				"recordEditor": {
					"type":  "cspace.recordEditor",
					"options": {
						"fieldsToIgnore": ["csid", "fields.csid", "fields.identificationNumber", "fields.createdAt", "fields.createdBy", "fields.updatedAt", "fields.updatedBy"],
						"selectors": {
							"identificationNumber": ".csc-media-imageNumber"
						},
						"originalMediaDimensions": {
							"width": "800",
							"height": "600"
						},
						"uispec": "{pageBuilder}.options.uispec.recordEditor",
						"components": {
							"recordRenderer": {
								"options": {
									"components": {
										"uploader": {
											"options": {
												"elPaths": {
													"required": "fields.imageNumber"
												}
											}
										}
									}
								}
							}
						}
					}
				},
				"titleBar": {
					"type": "cspace.titleBar",
					"options": {
						"fields": [
							"fields.identificationNumber",
							"fields.title"
						]
					}
				},
				"header": {
					"type": "cspace.header",
					"options": {
						"schema": "{pageBuilder}.schema",
						"permissions": "{pageBuilder}.permissions"
					}
				},
				"tabs": {
					"type": "cspace.tabs"
				},
				"sidebar": {
					"type": "cspace.sidebar"
				}
			}
		}
	},
	"pageBuilderIO": {
		"options": {
			"recordType": "media",
			"schema": {
				"media": null
			}
		}
	}
}