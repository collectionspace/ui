{
    "pageBuilder": {
        "options": {
            "pageType": "cataloging",
            "components": {
                "recordEditor": {
                    "type":  "cspace.recordEditor",
                    "options": {
                        "selectors": {
                        },
                        "uispec": "{pageBuilder}.options.uispec.recordEditor",
                        "fieldsToIgnore": ["csid", "fields.csid", "fields.objectNumber", "fields.narrowerContexts", "fields.computedCurrentLocation", "fields.computedCrate", "fields.createdAt", "fields.createdBy", "fields.updatedAt", "fields.updatedBy"]
                    }
                },
                "titleBar": {
                    "type": "cspace.titleBar",
                    "options": {
                        "fields": [
                            "fields.objectNumber",
                            {
                                "type": "repeatableMatch",
                                "queryPath": "fields.bampfaTitleGroup",
                                "childPath": "_primary",
                                "value": true,
                                "path": "bampfaTitle"
                            }, {
                                "type": "repeatableMatch",
                                "queryPath": "fields.objectProductionDateGroup",
                                "childPath": "_primary",
                                "value": true,
                                "path": "dateDisplayDate"
                            }, {
                                "type": "repeatableMatch",
                                "queryPath": "fields.bampfaObjectProductionPersonGroup",
                                "childPath": "_primary",
                                "value": true,
                                "path": "bampfaObjectProductionPerson"
                            }
                        ]
                    }
                },
                "header": {
                    "type": "cspace.header",
                    "options": {
                        "schema": "{pageBuilder}.schema",
                        "permissions": "{pageBuilder}.permissions"
                    }
                },
                "tabs": {
                    "type": "cspace.tabs"
                },
                "sidebar": {
                    "type": "cspace.sidebar"
                }
            }
        }
    },
    "pageBuilderIO": {
        "options": {
            "recordType": "cataloging",
            "schema": {
                "cataloging": null
            }
        }
    }
}