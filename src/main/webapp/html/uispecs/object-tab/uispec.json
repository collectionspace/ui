        ".csc-object-description-physical-description": "${fields.physicalDescription}",
        ".csc-object-description-inscription-description-type": "${fields.inscriptionDescriptionType}",
        ".csc-object-identification-object-language": "${fields.objectNameLanguage}",
        ".csc-object-description-inscription-content-interpretation": "${fields.inscriptionInterpretation}",
        ".csc-object-description-material-component": "${fields.materialComponent}",
        ".csc-object-description-phase": "${fields.phase}",
        ".csc-object-description-inscription-content-type": "${fields.inscriptionType}",
        ".csc-object-description-inscription-description-date": "${fields.inscriptionDescriptionDate}",
        ".csc-object-description-content-people": "${fields.contentPeople}",
        ".csc-object-description-content-person-autocomplete": {
            "decorators": [
                {
                    "func": "cspace.autocomplete",
                    "type": "fluid",
                    "options": {
                        "queryUrl": "../../chain/objects/autocomplete/contentPerson",
                        "vocabUrl": "../../chain/objects/source-vocab/contentPerson"
                    }
                }
            ]
        },
        ".csc-object-identification-object-title-translation": "${fields.titleTranslation}",
        ".csc-object-description-content-object-type": "${fields.contentObjectType}"
