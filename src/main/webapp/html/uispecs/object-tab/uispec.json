            "decorators": [
                {
                    "func": "cspace.autocomplete",
                    "type": "fluid",
                    "options": {
                        "queryUrl": "../../chain/objects/autocomplete/contentPeople",
                        "vocabUrl": "../../chain/objects/source-vocab/contentPeople"
                    }
                }
            ]
        },
        ".csc-object-description-inscription-description-position": "${fields.inscriptionDescriptionPosition}",
        ".csc-object-description-inscription-content-transliteration": "${fields.inscriptionTransliteration}",
