        ".csc-object-description-edition-number": "${fields.editionNumber}",
        ".csc-object-description-content-concept": "${fields.contentConcept}",
        ".csc-object-description-content-note": "${fields.contentNote}",
        ".csc-object-description-material": "${fields.material}",
        ".csc-object-description-material-component-note": "${fields.materialComponentNote}",
        ".csc-date-information-date-association": "${fields.dateAssociation}",
