{
    "list": {
        ".csc-recordList-row:": {
            "children": [
                {
                    ".csc-related-number": "${items.0.number}",
                    ".csc-related-summary": "${items.0.summary}"
                }
            ]
        }
    },
    "details": {
        ".csc-object-description-inscription-content-inscriber": "${fields.inscriber}",
        ".csc-date-information-date-period": "${fields.datePeriod}",
        ".csc-object-description-content-organization": "${fields.contentOrganization}",
        ".csc-object-description-inscription-content-translation": "${fields.inscriptionTranslation}",
        ".csc-object-description-content-place": "${fields.contentPlace}",
