        ".csc-object-identification-object-system": "${fields.objectNameSystem}",
        ".csc-object-description-style": "${fields.style}",
        ".csc-object-description-content-other": "${fields.contentOther}",
        ".csc-object-description-content-position": "${fields.contentPosition}",
        ".csc-object-description-dimension-value": "${fields.dimensionValue}",
        ".csc-object-description-content-person": "${fields.contentPerson}",
        ".csc-object-description-status": "${fields.objectStatus}",
        ".csc-object-identification-distinguishing-features": "${fields.distinguishingFeatures}",
        ".csc-date-information-date-earliest-single-qualifier": "${fields.dateEarliestSingleQualifier}",
        ".csc-object-description-dimension-unit": {
            "optionnames": [
                "centimeters",
                "millimeters",
                "meters",
                "liters",
                "kilograms",
                "cubic centimeters",
                "minutes",
                "carats",
                "pixels",
                "count",
                "stories",
                "inches",
                "square feet",
                "feet"
            ],
            "optionlist": [
                "centimeters",
                "millimeters",
                "meters",
                "liters",
                "kilograms",
                "cubic-centimeters",
                "minutes",
                "carats",
                "pixels",
                "count",
                "stories",
                "inches",
                "square-feet",
                "feet"
            ],
            "selection": "${fields.dimensionMeasurementUnit}"
        },
        ".csc-object-description-inscription-content-date": "${fields.inscriptionDate}",
        ".csc-object-identification-comments": "${fields.comments}",
        ".csc-object-description-object-component-information": "${fields.objectComponentInformation}",
        ".csc-object-identification-brief-description": "${fields.briefDescription}",
        ".csc-object-description-content-description": "${fields.contentDescription}",
        ".csc-object-identification-object-number-container": {
            "decorators": [
                {
                    "func": "cspace.numberPatternChooser",
                    "type": "fluid",
                    "options": {
                        "model": {
                            "names": [
                                "Intake",
                                "Accession",
                                "Loans In"
                            ],
                            "list": [
                                "intake",
                                "objects",
                                "loans-in"
                            ],
                            "samples": [
                                "IN2009.1.1",
                                "2009.1.1",
                                "LI2009.1.1"
                            ]
                        },
                        "selectors": {
                            "numberField": ".csc-object-identification-object-number"
                        }
                    }
                }
            ]
        },
        ".csc-object-description-age-unit": "${fields.ageUnit}",
        ".csc-object-identification-object-name": "${fields.objectName}",
        ".csc-object-description-content-script": "${fields.contentScript}",
        ".csc-object-description-dimension": "${fields.dimension}",
        ".csc-object-description-object-component-name": "${fields.objectComponentName}",
        ".csc-object-description-inscription-description-method": "${fields.inscriptionDescriptionMethod}",
        ".csc-date-information-date-latest-qualifier": "${fields.dateLatestQualifier}",
