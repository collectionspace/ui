            "optionnames": [
                "Antiquities",
                "Architecture and Design",
                "Decorative Arts",
                "Ethnography",
                "Herpetology",
                "Media and Performance Art",
                "Paintings and Sculpture",
                "Paleobotany",
                "Photographs",
                "Prints and Drawings"
            ],
            "optionlist": [
                "antiquities",
                "architecture-design",
                "decorative-arts",
                "ethnography",
                "herpetology",
                "media-performance-art",
                "paintings-sculpture",
                "paleobotany",
                "photographs",
                "prints-drawings"
            ],
            "selection": "${fields.responsibleDepartment}"
        },
        ".csc-object-description-technical-attribute": "${fields.technicalAttribute}",
        ".csc-object-description-material-name": "${fields.materialName}",
        ".csc-object-identification-other-number": "${fields.otherNumber}",
        ".csc-object-description-sex": "${fields.sex}",
        ".csc-object-description-inscription-description": "${fields.inscriptionDescription}",
