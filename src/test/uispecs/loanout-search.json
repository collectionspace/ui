    },
    ".csc-loanOut-loanedObjectStatus": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "loanStatusGroup",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-loanOut-loanedObjectStatus": {
                                    "decorators": [
                                        {
                                            "func": "cspace.termList",
                                            "options": {
                                                "elPath": "loanStatus",
                                                "root": "{row}",
                                                "termListType": "loanStatus"
                                            },
                                            "type": "fluid"
                                        }
                                    ]
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
