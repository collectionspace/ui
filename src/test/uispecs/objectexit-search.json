                    "elPath": "exitMethods",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-objectexit-exitMethods": {
                                    "default": "",
                                    "optionlist": [
                                        "",
                                        "courier",
                                        "inperson",
                                        "post"
                                    ],
                                    "optionnames": [
                                        "Please select a value",
                                        "Courier",
                                        "In Person",
                                        "Post"
