{
    ".csc-loanIn-loanInNumber": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "loanInNumbers",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-loanIn-loanInNumber": "${{row}.loanInNumber}"
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-loanIn-loanPurpose": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "loanPurposes",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-loanIn-loanPurpose": {
                                    "default": "",
                                    "optionlist": [
                                        "",
                                        "exhibition",
                                        "research",
                                        "scientificorexhibitpreparation",
                                        "analysis",
                                        "photography",
                                        "conservationotherrequestedservices",
                                        "longtermcollectionsmanagementandstorage"
                                    ],
                                    "optionnames": [
                                        "Please select a value",
                                        "Exhibition",
                                        "Research",
                                        "Scientific or exhibit preparation",
                                        "Analysis",
                                        "Photography",
                                        "Conservation or other requested services",
                                        "Long-term collections management and storage"
                                    ],
                                    "selection": "${{row}.loanPurpose}"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-loanin-lender-label": {
        "messagekey": "loanin-lenderLabel"
    },
    ".csc-loanin-lenderGroup": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "lenderGroup",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-loanIn-lender": {
                                    "decorators": [
                                        {
                                            "func": "cspace.autocomplete",
                                            "options": {
                                                "queryUrl": "../../../tenant/core/loanin/autocomplete/lender",
                                                "vocabUrl": "../../../tenant/core/loanin/source-vocab/lender"
                                            },
                                            "type": "fluid"
                                        }
                                    ],
                                    "value": "${{row}.lender}"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-loanin-lendersAuthorizationDate-label": {
        "messagekey": "loanin-lendersAuthorizationDateLabel"
    },
    ".csc-loanin-lendersAuthorizer-label": {
