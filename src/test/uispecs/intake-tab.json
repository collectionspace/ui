                                                "optionnames": [
                                                    "Please select a value",
                                                    "Suitable",
                                                    "Reasonable",
                                                    "Unsuitable",
                                                    "Dangerous"
                                                ],
                                                "optionlist": [
                                                    "",
                                                    "suitable",
                                                    "reasonable",
                                                    "unsuitable",
                                                    "dangerous"
                                                ],
                                                "selection": "${{row}.currentLocationFitness}"
                                            },
                                            ".csc-intake-current-location-note": "${{row}.currentLocationNote}"
                                        },
                                        "type": "fluid.renderer.repeat",
                                        "pathAs": "row",
                                        "controlledBy": "fields.currentLocationGroup"
