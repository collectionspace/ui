{
    ".csc-location-accessNote": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "accessNotes",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-location-accessNote": "${{row}.accessNote}"
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-location-accessNote-label": {
        "messagekey": "location-accessNoteLabel"
    },
    ".csc-location-address": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "addresss",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-location-address": "${{row}.address}"
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-location-address-label": {
        "messagekey": "location-addressLabel"
    },
    ".csc-location-conditionGroup": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "conditionGroup",
                    "hidePrimary": true,
