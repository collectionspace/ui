    ".csc-preferredOA-orgTermGroup": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "orgTermGroup",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-orgAuthority-termDisplayName": "${{row}.termDisplayName}",
                                ".csc-orgAuthority-termStatus": {
                                    "default": "",
                                    "optionlist": [
                                        "provisional",
                                        "under review",
                                        "accepted",
                                        "rejected",
                                        ""
                                    ],
                                    "optionnames": [
                                        "Provisional",
                                        "Under Review",
                                        "Accepted",
                                        "Rejected",
                                        "Please select a value"
                                    ],
                                    "selection": "${{row}.termStatus}"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
