    ".csc-orgAuthority-foundingPlace": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "foundingPlaces",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-orgAuthority-foundingPlace": "${{row}.foundingPlace}"
                            },
                            "type": "fluid.noexpand"
