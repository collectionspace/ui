                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-movement-frequencyForInventory": {
                                    "default": "",
                                    "optionlist": [
                                        "",
                                        "daily",
                                        "weekly",
                                        "monthly",
                                        "semi-annually",
                                        "annually"
                                    ],
                                    "optionnames": [
                                        "Please select a value",
                                        "Daily",
                                        "Weekly",
                                        "Monthly",
                                        "Semi-annually",
                                        "Annually"
                                    ],
                                    "selection": "${{row}.frequencyForInventory}"
