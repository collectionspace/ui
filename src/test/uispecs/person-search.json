    ".csc-person-updatedBy": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "updatedBys",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-person-updatedBy": "${{row}.updatedBy}"
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-person-updatedBy-label": {
        "messagekey": "person-updatedByLabel"
    },
    ".csc-personAuthority-gender": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "genders",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-personAuthority-gender": {
                                    "default": "",
                                    "optionlist": [
                                        "",
                                        "male",
                                        "female"
                                    ],
                                    "optionnames": [
                                        "Please select a value",
                                        "Male",
                                        "Female"
                                    ],
                                    "selection": "${{row}.gender}"
                                }
                            },
                            "type": "fluid.noexpand"
                        }
                    }
                },
                "type": "fluid"
            }
        ]
    },
    ".csc-personAuthority-group": {
        "decorators": [
            {
                "func": "cspace.makeRepeatable",
                "options": {
                    "elPath": "groups",
                    "hidePrimary": true,
                    "repeatTree": {
                        "expander": {
                            "tree": {
                                ".csc-personAuthority-group": "${{row}.group}"
                            },
