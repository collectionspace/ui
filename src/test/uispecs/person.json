                                                                    "optionnames": ["Please select a value", "descriptor", "alternate descriptor", "used for term"],
                                                                    "optionlist": ["", "descriptor", "alternate descriptor", "used for term"],
                                                                    "selection": "${{row}.termType}"
                                                                },
                                                                ".csc-personAuthority-termStatus": {
                                                                    "default": "provisional",
                                                                    "optionnames": [
                                                                        "Provisional",
                                                                        "Under Review",
                                                                        "Accepted",
                                                                        "Rejected"
                                                                    ],
                                                                    "optionlist": [
                                                                        "provisional",
                                                                        "under review",
                                                                        "accepted",
                                                                        "rejected"
                                                                    ],
                                                                    "selection": "${{row}.termStatus}"
                                                                },
                                                                ".csc-personAuthority-termLanguage": {
                                                                    "decorators": [{
                                                                        "func": "cspace.termList",
                                                                        "type": "fluid",
                                                                        "options": {
                                                                            "elPath": "${{row}.termLanguage}",
                                                                            "termListType": "termLanguage"
                                                                        }
                                                                    }]
                                                                },
                                                                ".csc-personAuthority-termPrefForLang": "${{row}.termPrefForLang}",
                                                                ".csc-personAuthority-termSource": "${{row}.termSource}",
                                                                ".csc-personAuthority-termSourceDetail": "${{row}.termSourceDetail}",
                                                                ".csc-personAuthority-termSourceID": "${{row}.termSourceID}",
                                                                ".csc-personAuthority-termSourceNote": "${{row}.termSourceNote}",
                                                                ".csc-personAuthority-salutation": {
                                                                    "default": "dear",
                                                                    "optionnames": ["Dear", "Hello", "To"],
                                                                    "optionlist": ["dear", "hello", "to"],
                                                                    "selection": "${{row}.salutation}"
                                                                },
                                                                ".csc-personAuthority-title": {
                                                                    "default": "",
                                                                    "optionnames": ["Please select a value", "Mr", "Mrs", "Ms", "Miss", "Dr", "Professor", "Sir", "Dame", "Baron", "Baroness", "Lord", "Lady"],
                                                                    "optionlist": ["", "Mr", "Mrs", "Ms", "Miss", "Dr", "Professor", "Sir", "Dame", "Baron", "Baroness", "Lord", "Lady"],
                                                                    "selection": "${{row}.title}"
                                                                },
                                                                ".csc-personAuthority-foreName": "${{row}.foreName}",
                                                                ".csc-personAuthority-middleName": "${{row}.middleName}",
                                                                ".csc-personAuthority-surName": "${{row}.surName}",
                                                                ".csc-personAuthority-nameAdditions": "${{row}.nameAdditions}",
                                                                ".csc-personAuthority-initials": "${{row}.initials}"
                                                            },
                                                            "type": "fluid.noexpand"
                                                        }
                                                    },
                                                    "elPath": "fields.preferredPersonGroup"
                                                }
                                            }
                                        ]
                                    },
                                    ".csc-person-termDisplayName-label": {
                                        "messagekey": "person-termDisplayNameLabel"
                                    },
                                    ".csc-person-termQualifier-label": {
                                        "messagekey": "person-termQualifierLabel"
                                    },
                                    ".csc-person-termType-label": {
                                        "messagekey": "person-termTypeLabel"
                                    },
                                    ".csc-person-termStatus-label": {
                                        "messagekey": "person-termStatusLabel"
                                    },
                                    ".csc-person-termLanguage-label": {
                                        "messagekey": "person-termLanguageLabel"
                                    },
                                    ".csc-person-termPrefForLang-label": {
