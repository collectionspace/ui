                                    ".csc-person-termSourceID-label": {
                                        "messagekey": "person-termSourceIDLabel"
                                    },
                                    ".csc-person-termSourceNote-label": {
                                        "messagekey": "person-termSourceNoteLabel"
                                    },
                                    ".csc-person-salutation-label": {
                                        "messagekey": "person-salutationLabel"
                                    },
                                    ".csc-person-title-label": {
                                        "messagekey": "person-titleLabel"
